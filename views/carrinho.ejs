    var urlBuyProductFromToLaterBasket = 'https://chk.paodeacucar.com/basket/addProduct';


    function checkoutFunction(){

        $('#basketForm').attr('action', urlUpdateBasket);

         //Adicionando dois parametros no request, o checkout e o comment
         if(!$('#field-info').is(":checked")){
            $('#basketComment').val('');
         }

         $('#checkoutBasketForm').val('true');

         //Submeterá para
         $("#basketForm").submit();

    }


    function createList(message) {
        if($("#listNameId").val()){
          $("#formCreateList").submit();
        }else{
          ep.message.showErrorMessage(message);
          //$("#listCreatedMessage").replaceWith('<div style="color: red; font-weight: bold;">'+ message +'</div>');
        }

    }

    function addCoupon(){
        var coupon = $("#basketCoupon").val();
        $.ajax({
            url:  urlBasketAddCoupon + "?coupon=" + coupon,
            success: function(data) {
                updateStoreProduct(data);
                //ep.message.showSuccessMessage("Cupom adicionado com sucesso.");
            }
        });
    }


    $(function() {


         $("#basketCenter").on('blur', '.quantityListener' , function(e) {
            var prodBasketId = $(this).attr("id").split("quantity_")[1];
            var value = $(this).val();
            changeProductQuantity(prodBasketId, value);
            return false;
        });

        $("#toLaterBasketContainer").on('click', '.addProductFromToLaterBasket', addProductFromToLaterBasket);

        $("#basketCenter").on('click', '.reloadListener', function() {
            var valueId = $(this).attr("id").split("reloadQuantity_")[1];
            var input = $('#quantity_' + valueId);
            var prodBasketId = valueId;
            var value = input.val();
            changeProductQuantity(prodBasketId, value);
            return false;
        });

        $("#basketCenter").on('click', '.removeListener', function(e){removeProduct(e, $(this))});

        //action de frete
        $("#basketCenter").on('click', '#changeZipCode' ,function() {
            var zipCode = $("#addressZipCode").val();
            $.ajax({
                url: urlBasketChangeZipCode+"?zipCode=" + zipCode,
                success: function(data) {
                    updateStoreProduct(data);
                }
            });
        });

        $("#basketCenter" ).on('click', '#emptyBasket', function(e) {

            var resp = confirm("Deseja realmente limpar todo o seu carrinho?");

            if (!resp) {
                e.preventDefault();
                return false;
            }

        });

        $("#basketCenter").on('change', '#sortBasket' ,function(){
            $.ajax({
                url: urlSortBasket+"?sort=" + $("#sortBasket option:selected").val(),
                success: function(data) {
                    updateStoreProduct(data);
                }
            });
        });

        $("#basketCenter").on('click', '#createListBasket' , function() {
            var id = $("#basketExisting").val();
            $.ajax({
                url: urlBasketSaveBasketInShoppingList + "?id=" + id,
                success: function(data) {
                    updateStoreProduct(data);
                    ep.message.showSuccessMessage("Carrinho salvo com sucesso.");
                }
            });

        });



        $("#basketCoupon").keydown(function (e){
            if(e.keyCode == 13){
            e.preventDefault();
                addCoupon();
                return false;
            }
        });


        $("#basketCenter").on('click', '#addCoupon', function() {
            addCoupon();
        });

        $("#basketCenter").on('click', "#doCheckout", function(event) {
            event.preventDefault();

            showOverlayWithLoading();

            checkoutFunction();
        });

        $("#basketCenter").on('click', "#elemFinalizarCompra a" ,function(event) {

            event.preventDefault();

            checkoutFunction();

        });

        $("#basketCenter").on('click', '#confirmBtnList' , function(e) {
            e.preventDefault();
            createList('Informe o nome da lista a ser criada.');
        });

        $("#basketCenter").on('click', '.updateAllBasket', updateAllBasketInfos);

        $("#basketCenter").on('click', '#markRemoveAll', function() {

            $('.deleteItem').prop('checked', $('#markRemoveAll').is(':checked'));

        });

        $("#basketCenter").on('click', '#markSimilarAll', function() {

            $('.similarItem').prop('checked', $('#markSimilarAll').is(':checked'));

        });



        $("#basketCenter").on("click", '.similarItem', function(e){

            var prodOnBasketId = $(this).closest('tr').attr('id').split('_')[1]
            var value = false;

            if($(this).is(':checked')){
                value = true;
            }

            var ret = acceptSimilarProduct(prodOnBasketId, value, $('.similarItem'));

        });

        $("#basketCenter").on("click", '.inc', function(e){
            var parent = $(this).parent().find('input');
            qtdChanger(true , parent); e.preventDefault();
            var prodBasketId = parent.attr("id").split("quantity_")[1];
            var value = parent.val();
            changeProductQuantity(prodBasketId, value);
            return false;
        });

        $("#basketCenter").on("click", '.dec', function(e){
            var parent = $(this).parent().find('input');
            qtdChanger(false ,$(this).parent().find('input')); e.preventDefault();
            var prodBasketId = parent.attr("id").split("quantity_")[1];
            var value = parent.val();
            changeProductQuantity(prodBasketId, value);
            return false;
        });


        $(".quantityListener").setMask({mask:"99999999"});
        $("#addressZipCode").setMask({mask:"99999-999"});

    });


</script>


